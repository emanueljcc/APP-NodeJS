doctype html
html(lang='es')
	head
		meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
		meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1.0')
		title Agence
		//- link(href='https://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
		link(rel='shortcut icon', href="img/favicon.ico", type='image/x-icon')
		link(rel='stylesheet', href="css/font-awesome/css/font-awesome.min.css")
		link(href="css/materialize.min.css", type='text/css', rel='stylesheet', media='screen,projection')
		link(href="css/style.css", type='text/css', rel='stylesheet', media='screen,projection')
		link(href="select2/css/select2.css", rel='stylesheet')
		link(href="select2/css/select2.min.css", rel='stylesheet')
		link(href="css/animate.css", rel='stylesheet')
	body
		nav
			.nav-wrapper.grey.lighten-5
				a.brand-logo.right(href='/')
					img#logo(src="img/logo.gif", alt='agence')
				ul.left.hide-on-med-and-down
					li
						a(href='#')
							i.fa.fa-home(aria-hidden='true')
							|  Agence
					li
						a(href='#')
							i.fa.fa-check-square-o(aria-hidden='true')
							|  Projetos
					li
						a(href='#')
							i.fa.fa-pencil-square-o(aria-hidden='true')
							|  Administrativo
					li.active
						a(href="/")
							i.fa.fa-users(aria-hidden='true')
							|  Comercial
					li
						a(href='#')
							i.fa.fa-list-alt(aria-hidden='true')
							|  Financeiro
					li
						a(href='#')
							i.fa.fa-user-circle-o(aria-hidden='true')
							|  Usuario
					li
						a(href='#')
							i.fa.fa-times(aria-hidden='true')
							|  Salir
				ul#nav-mobile.side-nav
					li
						a(href='#')
							i.fa.fa-home(aria-hidden='true')
							|  Agence
					li
						a(href='#')
							i.fa.fa-check-square-o(aria-hidden='true')
							|  Projetos
					li
						a(href='#')
							i.fa.fa-pencil-square-o(aria-hidden='true')
							|  Administrativo
					li.active
						a(href="/")
							i.fa.fa-users(aria-hidden='true')
							|  Comercial
					li
						a(href='#')
							i.fa.fa-list-alt(aria-hidden='true')
							|  Financeiro
					li
						a(href='#')
							i.fa.fa-user-circle-o(aria-hidden='true')
							|  Usuario
					li
						a(href='#')
							i.fa.fa-times(aria-hidden='true')
							|  Salir
				a.button-collapse(href='#', data-activates='nav-mobile')
					i.fa.fa-bars.fa-2
		main.mn-inner
			block content
		script(src="js/jquery-3.2.0.min.js")
		script(src="js/materialize.min.js")
		script(src="js/init.js")
		script(src="js/script.js")
		script(src="select2/js/select2.min.js")
		script(src="pages/form-select2.js")
		script(type='text/javascript').
			$(document).ready(function() {
			$('select').material_select();
			$('img').fadeIn(4000);
			$( ".mn-inner" ).slideDown(1000);
			});
		
